      },
      "longitude": {
        "type": "float",
        "default": 2.3522
      },
      "zoom": {
        "type": "integer",
        "default": 15
      },
      "height": {
        "type": "integer",
        "default": 400
      },
      "style": {
        "type": "string",
        "default": "default"
      },
      "className": {
        "type": "string"
      }
    },
    "useOnce": true
  },
