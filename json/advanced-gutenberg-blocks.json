[
	{
    "name": "advanced-gutenberg-blocks/notice",
    "title": "Notice",
    "description": "Put forward a tips or a warning",
    "category": "common",
    "icon": {
      "background": "#D56561",
      "foreground": "#fff",
      "src": "warning",
      "shadowColor": "rgba(213, 101, 97, 0.3)"
    },
    "keywords": [
      "warning",
      "information",
      "tips"
    ],
    "attributes": {
      "type": {
        "source": "attribute",
        "selector": ".wp-block-advanced-gutenberg-blocks-notice",
        "attribute": "data-type",
        "default": "advice"
      },
      "title": {
        "source": "text",
        "type": "string",
        "selector": ".wp-block-advanced-gutenberg-blocks-notice__title",
        "default": "Advice"
      },
      "content": {
        "type": "array",
        "source": "children",
        "selector": ".wp-block-advanced-gutenberg-blocks-notice__content"
      },
      "className": {
        "type": "string"
      }
    }
  },
