  {
    "name": "advanced-gutenberg-blocks/gmap",
    "title": "Google Map",
    "description": "Display a customizable Google map",
    "category": "common",
    "icon": {
      "background": "#D56561",
      "foreground": "#fff",
      "src": "location-alt",
      "shadowColor": "rgba(213, 101, 97, 0.3)"
    },
    "keywords": [
      "gmap"
    ],
    "attributes": {
      "address": {
        "type": "string",
        "default": "Paris"
      },
      "name": {
